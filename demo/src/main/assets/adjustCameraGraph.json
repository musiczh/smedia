{
  "services": [
    {
      "service": "GLContext",
      "name": "GLContext"
    }
  ],
  "executors": [
    {
      "options": {
        "threadNum": 1
      }
    }
  ],
  "nodes": [
    {
      "functor": "OESTexReaderFunctor",
      "name": "oesNode",
      "outputs": [
        "video:v1"
      ]
    },
    {
      "functor": "ImageSourceFunctor",
      "name": "imageSourceNode",
      "options": {
        "glEnable": true
      },
      "outputs": [
        "video:video1",
        "video:video_dispatch"
      ]
    },
    {
      "functor": "AdjustColorFunctor",
      "name": "adjustNode",
      "inputs": [
        "video:v1",
        "video:video1"
      ],
      "outputs": [
        "video:v2"
      ]
    },
    {
      "functor": "HighLightShadowFunctor",
      "name": "shadowNode",
      "inputs": [
        "video:v2"
      ],
      "outputs": [
        "video:v3"
      ]
    },
    {
      "functor": "SharpenFunctor",
      "name": "sharpenNode",
      "inputs": [
        "video:v3"
      ],
      "outputs": [
        "video:v4"
      ]
    },
    {
      "functor": "WhiteBalanceFunctor",
      "name": "whiteBalanceNode",
      "inputs": [
        "video:v4"
      ],
      "outputs": [
        "video:v5"
      ]
    },
    {
      "functor": "WhiteBalanceFunctor",
      "name": "whiteBalanceNode2",
      "inputs": [
        "video:v5"
      ],
      "outputs": [
        "video:v6"
      ]
    },
    {
      "functor": "DispatchFunctor",
      "name": "dispatchNode",
      "inputs": [
        "master:v6",
        "image:video_dispatch"
      ],
      "outputs": [
        "master:m",
        "image:image_dispatch"
      ]
    },
    {
      "functor": "RenderFunctor",
      "name": "renderNode",
      "inputs": [
        "video:m"
      ]
    },
    {
      "functor": "GLImageReaderFunctor",
      "name": "imageReaderNode",
      "inputs": [
        "video:image_dispatch"
      ],
      "outputs": [
        "buffer:imageData"
      ]
    },
    {
      "functor": "ImageLoader",
      "name": "imageLoaderNode",
      "inputs": [
        "data:imageData"
      ],
      "outputs": [
        "data:bitmapData"
      ]
    },
    {
      "functor": "CallbackFunctor",
      "name": "callbackNode",
      "inputs": [
        "data:bitmapData"
      ]
    }
  ]
}